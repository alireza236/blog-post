{
     "presets": [
         ["@babel/preset-env", {
             "targets": {
                 "node": "current"
             }
         }]
     ],
     "plugins": [
         ["@babel/plugin-proposal-object-rest-spread", {
             "useBuiltIns": true
         }],
          ["@babel/plugin-transform-runtime", {
              "corejs": false,
              "helpers": true,
              "regenerator": true,
              "useESModules": false
          }]
     ]
 }